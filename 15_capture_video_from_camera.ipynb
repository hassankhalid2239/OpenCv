    "cap=cv2.VideoCapture(0)\n",
    "\n",
    "while True:\n",
    "    r,frame=cap.read()\n",
    "    if r== True:\n",
    "        cv2.imshow(\"Window\",frame)\n",
    "        if cv2.waitKey(25) & 0xff==ord(\"p\"):\n",
    "            break\n",
    "    else:\n",
    "        break\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()"
